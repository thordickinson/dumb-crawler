{
  "crawler": {
    "seeds": [
      "https://motos.mercadolibre.com.co/"
    ],
    "threadCount": 15,
    "urlFilter": "matches(host, '(moto|vehiculo)s?\\.mercadolibre\\.com\\.co') and not isResource(path)",
    "priorities": [
       {"urlFilter": "matches(path, '^\/(MCO|mco)-[0-9]+.*')", "priority": 100}
    ]
  },
  "rocketScrape": {
    "urlFilter": "matches(host, '(moto|vehiculo)s?\\.mercadolibre\\.com\\.co')"
  },
  "idUrlHasher": {
    "urlFilter": "matches(path, '^\/(MCO|mco)-[0-9]+.*' )",
    "idExtractor": "extract(path, '^\/(?<value>(MCO|mco)-\\d+)-.*' )"
  },
  "avroStorage": {
    "urlFilter": "matches(path, '^\/(MCO|mco)-[0-9]+.*')"
  },
  "taskKiller": {
    "timeout": "20m",
    "urlFilter": "matches(path, '^\/(MCO|mco)-[0-9]+.*')"
  },
  "timedTaskKiller": {
    "timeout": "5h"
  },
  "forceRenderContentValidator" : {
    "disabled": false,
    "urlFilter": "matches(path, '^\/(MCO|mco)-[0-9]+.*')",
    "validIf": "containsElement(document, 'div.ui-pdp-specs')"
  }
}