      },
      {
        "action": "accept",
        "field": "uri.host",
        "operator": "contains",
        "argument": "carro.mercadolibre."
      },
      {
        "action": "accept",
        "field": "uri.host",
        "operator": "contains",
        "argument": "vehiculo.mercadolibre."
      },
      {
        "action": "accept",
        "field": "uri.host",
        "operator": "contains",
        "argument": "carros.mercadolibre."
      },
      {
        "action": "accept",
        "field": "uri.host",
        "operator": "contains",
        "argument": "vehiculos.mercadolibre."
      },
      {
        "action": "reject",
        "field": "uri.path",
        "operator": "contains",
        "argument": "robots.txt"
      }
