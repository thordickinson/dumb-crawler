    "config": {
      "apiKey": "a8786555-59e6-4171-be58-804718a74a2a"
    }
  },
  "crawler": {
    "filters": [
      {
        "action": "reject"
      },
      {
        "field": "uri.host",
        "operator": "pattern",
        "argument": "^(carro|carro|vehiculo|vehiculos)\\.mercadolibre\\.com\\.co$",
        "action": "accept"
      },
      {
        "field": "uri.path",
        "operator": "pattern",
        "argument": ".*\\.(jpg|jpeg|css|js|png|webp|mpeg|mpg|mp3|mp4|ogg|wav|iso|avi)$",
        "action": "reject"
      }
    ]
  },
  "orcStorage": {
    "filters": [
      {
        "action": "reject"
      },
      {
        "action": "accept",
        "field": "uri.path",
        "operator": "pattern",
        "argument": "^\/(MCO|mco)-[0-9]+.*"
      }
    ]
  },
  "stopper": {
    "config": {
      "timeout": 600
    },
    "filters": [
      {
        "action": "reject"
      },
      {
        "action": "accept",
        "field": "uri.path",
        "operator": "pattern",
        "argument": "^\/(MCO|mco)-[0-9]+.*"
      }
    ]
  }
