{
  "seeds": [
    "https://www.ics.uci.edu/~lopes/",
    "https://www.ics.uci.edu/~welling/",
    "https://www.ics.uci.edu/"
  ],
  "threadCount": 3,
  "orcStorage": {},
  "rocketScrape": {
    "filters": [
      {
        "action": "reject"
      }
    ],
    "config": {
      "apiKey": "xx"
    }
  },
  "crawler": {
    "filters": [
      {
        "action": "reject"
      },
      {
        "field": "uri.host",
        "operator": "contains",
        "argument": ".edu",
        "action": "accept"
      },
      {
        "field": "uri.path",
        "operator": "pattern",
        "argument": ".*\\.(jpg|jpeg|css|js|png|webp|mpeg|mpg|mp3|mp4|ogg|wav|iso|avi)$",
        "action": "reject"
      }
    ]
  },
  "stopper": {
    "config": {
      "timeout": 10
    },
    "filters": [
      {
        "action": "accept"
      }
    ]
  }
}