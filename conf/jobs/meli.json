{
  "seeds": [
    "https://motos.mercadolibre.com.co/"
  ],
  "idExtractorPattern": "MCO-[0-9]+",
  "threadCount": 15,
  "tagger": {
    "specialTags": ["other"],
    "item": "matches(path, '^\/(MCO|mco)-[0-9]+.*')",
    "listing": "matches(host, '(moto|vehiculo)s?\\.mercadolibre\\.com\\.co') and not isResource(path)"
  },
  "priorities": {
    "seed": 1000,
    "item": 500,
    "listing": 100,
    "other": 10
  },
  "fetcher": {
    "proxied": ["item"],
    "render": ["listing"]
  },
  "storage": {
    "includeTags": ["item"]
  },
  "taskKiller": {
    "timeout": "20m"
  },
  "contentValidator" : {
    "disabled": false,
    "includedTags": ["items"],
    "validIf": "containsElement(document, 'div.ui-pdp-specs')"
  }
}